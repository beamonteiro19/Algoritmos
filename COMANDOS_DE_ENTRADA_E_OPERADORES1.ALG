Algoritmo "comandos_de_entrada_e_operadores1"

Var
   n1, n2, soma: inteiro
   


Inicio
      Escreval("Informe um número: ")
      Leia(n1)
      Escreval("Informe outro número: ")
      Leia(n2)
      soma <- n1+n2
      Escreval("A soma dos valores",n1, " e ", n2, " é: ",soma)
      
      


Fimalgoritmo