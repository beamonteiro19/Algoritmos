Algoritmo "comandos_de_entrada_e_operadores2"

Var
   n1, n2: inteiro
   media: real



Inicio
   Escreval("Informe um número: ")
   Leia(n1)
   Escreval("Informe outro número: ")
   Leia(n2)
   media <- (n1+n2)/ 2
   Escreval("A média dos valores ",n1, " e ", n2, " é: ",media)




Fimalgoritmo